<application>
  <component name="LanguageInjectionConfiguration">
    <option name="INSTRUMENTATION" value="ASSERT" />
    <option name="LANGUAGE_ANNOTATION" value="org.intellij.lang.annotations.Language" />
    <option name="PATTERN_ANNOTATION" value="org.intellij.lang.annotations.Pattern" />
    <option name="SUBST_ANNOTATION" value="org.intellij.lang.annotations.Subst" />
    <option name="RESOLVE_REFERENCES" value="true" />
    <injection language="JavaScript" injector-id="java">
      <display-name>AppController (com.yesikov.springmvc.controller)</display-name>
      <place disabled="true"><![CDATA[psiMethod().withName("getFilesList").withParameters("org.springframework.ui.ModelMap").definedInClass("com.yesikov.springmvc.controller.AppController")]]></place>
    </injection>
    <injection language="JAVA" injector-id="java">
      <display-name>PrintStream.println (java.io)</display-name>
      <place><![CDATA[psiParameter().ofMethod(0, psiMethod().withName("println").withParameters("java.lang.String").definedInClass("java.io.PrintStream"))]]></place>
    </injection>
    <injection language="AIDL" injector-id="xml">
      <display-name>LinearLayout/@context</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("context")).withNamespace(string().equalTo("http://schemas.android.com/tools")).withParent(xmlTag().withLocalName(string().equalTo("LinearLayout")))]]></place>
    </injection>
    <injection language="JavaScript" injector-id="xml">
      <display-name>outputScript/@name</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("name")).withParent(xmlTag().withLocalName(string().equalTo("outputScript")).withNamespace(string().equalTo("http://java.sun.com/jsf/html")))]]></place>
    </injection>
  </component>
</application>